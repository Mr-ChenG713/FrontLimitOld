<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.chengbo.frontlimit">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.FrontLimit">

        <activity android:name=".activity.CidadeActivity"
            android:parentActivityName=".activity.OutroActivity"/>
        <activity android:name=".activity.PaisActivity"
            android:parentActivityName=".activity.OutroActivity"/>
        <activity android:name=".activity.MercadoriaActivity"
            android:parentActivityName=".activity.OutroActivity"/>
        <activity android:name=".activity.CategoriaActivity"
            android:parentActivityName=".activity.OutroActivity"/>
        <activity android:name=".activity.FronteiraActivity"
            android:parentActivityName=".activity.OutroActivity"/>
        <activity android:name=".activity.OpcActivity"
            android:parentActivityName=".activity.OutroActivity"/>
        <activity android:name=".activity.MarcaActivity"
            android:parentActivityName=".activity.OutroActivity"/>

        <activity
            android:name=".activity.SobreActivity"
            android:parentActivityName=".activity.PrincipalActivity" />
        <activity
            android:name=".activity.OutroActivity"
            android:parentActivityName=".activity.PrincipalActivity" />
        <activity android:name=".activity.PrincipalActivity" />
        <activity
            android:name=".activity.AgentePerfilActivity"
            android:parentActivityName=".activity.PrincipalActivity" />
        <activity android:name=".activity.RegisteActivity" />
        <activity android:name=".activity.LoginActivity" />
        <activity android:name=".SplashScreenActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>